meta {
  name: 01 - Check first StudentHomelessProgramAssociation is valid
  type: http
  seq: 1
}

get {
  url: {{resourceBaseUrl}}/ed-fi/studentHomelessProgramAssociations?educationOrganizationId=[ENTER FIRST EDUCATION ORGANIZATION ID]&programEducationOrganizationId=[ENTER FIRST PROGRAM EDUCATION ORGANIZATION ID]&programTypeDescriptor={{firstProgramTypeDescriptorEncoded}}&programName=[ENTER FIRST PROGRAM NAME]&beginDate=[ENTER FIRST BEGIN DATE YYYY-MM-DD]&studentUniqueId=[ENTER FIRST STUDENT UNIQUE ID]&programTypeDescriptor_KEEP_IT_AT_THE_END=[ENTER FIRST PROGRAM TYPE DESCRIPTOR]
  body: none
  auth: inherit
}

params:query {
  educationOrganizationId: [ENTER FIRST EDUCATION ORGANIZATION ID]
  programEducationOrganizationId: [ENTER FIRST PROGRAM EDUCATION ORGANIZATION ID]
  programTypeDescriptor: {{firstProgramTypeDescriptorEncoded}}
  programName: [ENTER FIRST PROGRAM NAME]
  beginDate: [ENTER FIRST BEGIN DATE YYYY-MM-DD]
  studentUniqueId: [ENTER FIRST STUDENT UNIQUE ID]
  programTypeDescriptor_KEEP_IT_AT_THE_END: [ENTER FIRST PROGRAM TYPE DESCRIPTOR]
}

assert {
  res.status: eq 200
  res.body: isArray
  res.body: isNotEmpty
  res.body[0].id: isString
  res.body[0].id: isNotEmpty
  res.body[0].beginDate: isString
  res.body[0].beginDate: isNotEmpty
  res.body[0].educationOrganizationReference: isDefined
  res.body[0].educationOrganizationReference.educationOrganizationId: isNumber
  res.body[0].educationOrganizationReference.educationOrganizationId: neq 0
  res.body[0].programReference: isDefined
  res.body[0].programReference.educationOrganizationId: isNumber
  res.body[0].programReference.educationOrganizationId: neq 0
  res.body[0].programReference.programName: isString
  res.body[0].programReference.programName: isNotEmpty
  res.body[0].programReference.programTypeDescriptor: isString
  res.body[0].programReference.programTypeDescriptor: isNotEmpty
  res.body[0].studentReference: isDefined
  res.body[0].studentReference.studentUniqueId: isString
  res.body[0].studentReference.studentUniqueId: isNotEmpty
  res.body[0].homelessPrimaryNighttimeResidenceDescriptor: isString
  res.body[0].homelessPrimaryNighttimeResidenceDescriptor: isNotEmpty
  res.body[0].homelessUnaccompaniedYouth: isBoolean
}

script:pre-request {
  const { encodeDescriptorParameter, setVar } = require('./utils');
  const programTypeDescriptorEncoded = encodeDescriptorParameter(req.url, 'programTypeDescriptor_KEEP_IT_AT_THE_END');

  setVar(bru, 'firstProgramTypeDescriptorEncoded', programTypeDescriptorEncoded);
}

script:post-response {
  const { pickSingle, setVars, wipeVars, extractDescriptor } = require('./utils');
  const { logSpecStudentHomelessProgramAssociation, logScenario } = require('./logging');
  const entityName = 'StudentHomelessProgramAssociation';
  const scenarioName = this.req.name;
  const single = pickSingle(res.getBody());
  
  if (!single) {
    wipeVars(bru, [
      'firstStudentHomelessProgramAssociationUniqueId',
      'firstStudentHomelessProgramAssociationHomelessPrimaryNighttimeResidenceDescriptor'
    ], entityName, true);
  }
  
  setVars(bru, {
    firstStudentHomelessProgramAssociationUniqueId: single.id,
    firstStudentHomelessProgramAssociationHomelessPrimaryNighttimeResidenceDescriptor: extractDescriptor(single.homelessPrimaryNighttimeResidenceDescriptor)
  }, entityName);
  
  logScenario(entityName, scenarioName, single, logSpecStudentHomelessProgramAssociation);
}

settings {
  encodeUrl: false
  timeout: 0
}
