meta {
  name: Scenario 4 PUT - Get Grade
  type: http
  seq: 1
}

get {
  url: {{resourceBaseUrl}}/ed-fi/grades/{{s4GradeId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{certToken}}
}

script:pre-request {
  // Making the GET request to Grades and getting the ID of the grade
  await bru.sendRequest({
      url: `${bru.getEnvVar('resourceBaseUrl')}/ed-fi/grades`,
      method: 'GET',
      headers: {
          'Authorization': 'Bearer ' + bru.getEnvVar('certToken'),
          'Accept': '*/*'
      }
  }, async function(err, res) {
      if (err) {
          console.log('Error in the Authentication Process:', err);
      } else {
          var jsonData = res.data;
          let latestGrade = bru.findLatestByModifiedDate(jsonData);
  
          let latestGradeDate = latestGrade ? latestGrade._lastModifiedDate : null;
          console.log('latestGradeDate', latestGradeDate);
  
          bru.setEnvVar("s4GradeId", latestGrade.id);
          console.log('s4GradeId', bru.getEnvVar("s4GradeId"));
      }
  });
}

script:post-response {
  test("01 - Scenario 4 PUT: Test that Grade request has 200 code status", () => {
      expect(res.getStatus()).to.equal(200);
  });
  
  let response = res.getBody();
  
